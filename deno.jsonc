{
  "name": "@wei/probot-scheduler",
  "version": "0.1.0-alpha.1",
  "tasks": {
    "dev": "deno run --allow-all src/api/index.ts",
    "dev:skip-full-sync": "deno task dev --skip-full-sync",
    "dev:worker": "deno run --allow-all src/worker/index.ts",
    "full-sync": "deno run --allow-all scripts/full-sync.ts",
    "hook": "deno run --allow-read --allow-run --allow-write https://deno.land/x/deno_hooks@0.1.1/mod.ts"
  },
  "imports": {
    "@octokit/auth-app": "npm:@octokit/auth-app@^7.1.1",
    "@src/": "./src/",
    "@std/assert": "jsr:@std/assert@1",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.2",
    "@std/http": "jsr:@std/http@1",
    "@wei/pluralize": "jsr:@wei/pluralize@^8.0.1",
    "bullmq": "npm:bullmq@^5.20.0",
    "express": "npm:express@^4.21.1",
    "ioredis": "npm:ioredis@^5.4.1",
    "mongoose": "npm:mongoose@^8.7.1",
    "probot": "npm:probot@^13.3.8"
  },
  "exports": "./mod.ts"
}
